digraph {
	{ rank = source; 0; }
	{ rank = sink; 3; }
	0[shape=ellipse,label="[0] ENTRY"]
	1[shape=diamond,label="[1] try {\l}\lcatch (Exception e) {\l}\lfinally {\l}\l"]
	2[shape=box,label="[2] catch (Exception e) {\l}\l"]
	3[shape=ellipse,label="[3] EXIT"]
	0 -> 1
	1 -> 2 [label="F"]
	1 -> 3 [label="T"]
	2 -> 3
}
