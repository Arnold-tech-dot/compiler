m: output median of int;

p: Project = input;
cls_name : stack of string;

q_all := visitor {
	before decl : Declaration -> {
		push(cls_name,decl.name);
	}
	
	after node: Declaration -> {
		pop(cls_name);
	}

	before method: Method -> {
			cur_graph : ClientGraph;
			node_list : list of Node;
			cur_graph = getCfg(method,peek(cls_name));
			m << 1;
		}
};

visit(p, q_all);
