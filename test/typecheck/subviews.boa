view sv1 {
	p: Project = input;
	o1: output sum[string] of int;
	o2: output sum[string][string] of int;

	o1[p.id] << 1;
	o2[p.id][p.id] << 1;
}

view sv2 {
	view sv3 {
		o: output collection[string] of string;

		v: table [string] of count: int;
		#v: table [string] of count: int = J12345/o;
		r: v._row;

		while (v >> r) {
			o[r._1] << string(r.count);
		}
	}

	view sv4 {
		view sv5 {
			p: Project = input;

			o: output collection[int][string] of string;
			o[1][p.id] << p.id;
		}

		o: output collection[string][int] of string;
		o["haha"][1] << "foo";
		
		v := sv5/o;
		r : v._row;
	}

	o: output sum[int][int] of int;
	v1 := sv3/o;
	v2 := sv4/o;
	v3 := sv4/sv5/o;

	r1: v1._row;
	r2: v2._row;
	r3: v3._row;	
}

o: output sum[int][string] of int;

v1 := sv1/o1;
v2 := sv1/o2;
v3 := sv2/o;
v4 := sv2/sv3/o;
v5 := sv2/sv4/o;
v6 := sv2/sv4/sv5/o;

r1: v1._row;
r2: v2._row;
r3: v3._row;
r4: v4._row;
r5: v5._row;
r6: v6._row;


while(v1 >> r1) {
	o[r1._2][r1._1] << len(r1._1);
}
